	foundset.sort(asc)
}

function mySortFunction(r1, r2)
{
    var o = 0;
    if(r1.id < r2.id)
    {
        o = -1;
    }
    else if(r1.id > r2.id)
    {
        o = 1;
    }
    return o;
}
